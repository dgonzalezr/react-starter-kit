{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "Custom React Boilerplate",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.dev.js --open --history-api-fallback --env.PLATFORM=local --env.VERSION=stag",
    "prebuild": "webpack --config config/webpack.config.prod.js --env.PLATFORM=production --env.VERSION=stag --progress",
    "build": "serve ./dist -p 3001",
    "flow:start": "./node_modules/.bin/flow start",
    "flow:stop": "./node_modules/.bin/flow stop",
    "flow:status": "./node_modules/.bin/flow status --quiet",
    "flow:install": "./node_modules/.bin/flow-typed install -o --libdefDir .definitions"
  },
  "dependencies": {
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "styled-components": "4.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "8.0.5",
    "babel-plugin-module-resolver": "3.1.3",
    "babel-plugin-styled-components": "1.10.0",
    "copy-webpack-plugin": "4.6.0",
    "eslint": "5.13.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-flowtype": "3.2.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "flow-bin": "0.92.1",
    "flow-typed": "^2.5.1",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "1.16.4",
    "react-hot-loader": "4.6.5",
    "stylelint": "9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "1.5.2",
    "webpack": "4.29.3",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.2.1",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
